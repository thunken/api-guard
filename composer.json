{
    "name": "thunken/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/thunken/api-guard",
    "authors": [
        {
            "name": "Damien Vannson",
            "email": "damien@thunken.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Thunken\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "stable"
}
